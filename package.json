  "dependencies": {
    "commander": "^4.0.1",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "semistandard": "^14.2.0",
    "tape-es": "^1.1.4"

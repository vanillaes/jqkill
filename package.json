{
  "name": "jqkill",
  "version": "1.0.3",
  "description": "Locate and eliminate jQuery",
  "keywords": [
    "jquery",
    "kill",
    "utility",
    "cli"
  ],
  "repository": "https://github.com/vanillaes/jqkill.git",
  "author": "Evan Plaice <evanplaice@gmail.com> (http://evanplaice.com/)",
  "license": "MIT",
  "bin": {
    "jqkill": "bin/jqkill"
  },
  "type": "module",
  "engines": {
    "node": ">=13.2"
  },
  "scripts": {
    "test": "tape-es",
